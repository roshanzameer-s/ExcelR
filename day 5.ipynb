    "def sum_of_evens(n):\n",
    "    total = 0\n",
    "    for i in range(2, n+1, 2):\n",
    "        total += i\n",
    "    return total\n",
    "\n",
    "n = int(input())\n",
    "print(sum_of_evens(n))\n"
